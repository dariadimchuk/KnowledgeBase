<!-- Profile -->
<div class="centered">
    <a href="/profile/user/{{profile.profileID}}">
        <object data="{{profile.profileImage}}">
            <img src="/media/default-img.png" alt="Default Profile" />
        </object>
    </a>
    <h3>{{profile.firstName}} {{profile.lastName}}</h3>
    <p>{{profile.about}}</p>
</div>
<!--  Posts/Messages/Likes/Edit -->
<div class="post-message">
    <a href="/post">Posts</a><p><span id="posts">{{profile.numPosts}}</span></p><br>
    <a href="/message">Messages</a><p><span id="messages">{{profile.numMessages}}</span></p><br>
    <p>Likes<span id="likes">{{profile.numLikes}}</span></p><br><br>
    <a href="/profile/{{profile.profileID}}">Edit Profile</a>
</div>